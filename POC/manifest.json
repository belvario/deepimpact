//manifest.json
{

"manifest_version": 2,
"name": "DeepImpactPOC",
"description": "Super simple extension to look up JR for the journal site visited with the browser",
"version": "1.0",
"icons": {
 "128": "di-icon-128.png",
 "48": "di-icon-48.png",
 "16": "di-icon-16.png"
},
"browser_action": {
 "default_icon": "di-icon-16.png",
 "default_popup": "popup.html"
},
"content_scripts": [
 {
 "matches": ["<all_urls>"],
 "js": ["content.js"]
 }
],
"permissions": ["tabs"],
"background": {
 "scripts": ["background.js"]
},

"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
